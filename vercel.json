{
  "description": "Configuration updated to fix caching issues - triggering new deployment",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [".next/cache/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Cache-Control": "no-store, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/side-quests/foodtree/api/(.*)",
      "dest": "/api/foodtree/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false
}
